{
    "schemaVersion": "1.7.3",
    "acapPackageConf": {
        "setup": {
            "appName": "sensor_to_text",
            "friendlyName": "Sensor to Text App",
            "vendor": "Markus Svensson",
            "embeddedSdkVersion": "3.0",
            "runMode": "never",
            "version": "1.0.2"
        },
        "configuration": {
            "paramConfig": [
                {"name": "Sensor_Ip", "default": "0.0.0.0", "type": "string"},
                {"name": "Sensor_User", "default": "root", "type": "string"},
                {"name": "Sensor_Password","default": "pass", "type": "string"},
                {"name": "Text_Display_Ip","default": "0.0.0.0","type": "string"},
                {"name": "Text_Display_User","default": "root","type": "string"},
                {"name": "Text_Display_Password","default": "pass","type": "string"},
                {"name": "Show_Temperature","default": "yes","type": "bool:no,yes"},
                {"name": "Show_Humidity","default": "yes","type": "bool:no,yes"},
                {"name": "Show_CO2","default": "yes","type": "bool:no,yes"},
                {"name": "Show_NOx","default": "yes","type": "bool:no,yes"},
                {"name": "Seconds_Between_Cycles", "type": "int:min=0,max=3600", "default": "120"},
                {"name": "Seconds_Per_Data", "type": "int:min=0,max=100", "default": "5"}
            ]
        }
    }
}
